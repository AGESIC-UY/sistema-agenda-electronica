<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE jboss PUBLIC
  "-//JBoss//DTD JBOSS 5.0//EN"
  "http://www.jboss.org/j2ee/dtd/jboss_5_0.dtd">

<jboss>
	<security-domain>@SAE_SECURITY_DOMAIN@</security-domain> 
	<webservices>		
		<context-root>@SAE_WS_CONTEXT_ROOT@</context-root>
	</webservices>
	
	<enterprise-beans>
		<session>
			<ejb-name>ValidacionMailBean</ejb-name>
			<jndi-name>@SAE_EJB_JNDI_BASE@/ValidacionMailBean/remote</jndi-name>
			<local-jndi-name>@SAE_EJB_JNDI_BASE@/ValidacionMailBean/local</local-jndi-name>
		</session>
		<session>
			<ejb-name>ValidadorDatosPersonaBean</ejb-name>
			<jndi-name>@SAE_EJB_JNDI_BASE@/ValidadorDatosPersonaBean/remote</jndi-name>
			<local-jndi-name>@SAE_EJB_JNDI_BASE@/ValidadorDatosPersonaBean/local</local-jndi-name>
		</session>
		<session>
			<ejb-name>AccionNotificarBean</ejb-name>
			<jndi-name>@SAE_EJB_JNDI_BASE@/AccionNotificarBean/remote</jndi-name>
			<local-jndi-name>@SAE_EJB_JNDI_BASE@/AccionNotificarBean/local</local-jndi-name>
		</session>
	</enterprise-beans>
	
</jboss>